		{
			"enable": true,
			"name": "去除百度推广小尾巴",
			"ruleType": "redirect",
			"matchType": "regexp",
			"pattern": "(^https?):\\/\\/(www1?\\.baidu\\.com\\/)(?:s|baidu)\\?.*((?:wd=.*&pn=[^&]+)|(?:wd=[^&]+(?!&pn.*))).*$",
			"exclude": "",
			"isFunction": false,
			"action": "redirect",
			"to": "$1://$2s?$3",
			"group": "杂项"
		},
		{
			"enable": true,
			"name": "拦截高速下载器",
			"ruleType": "cancel",
			"matchType": "regexp",
			"pattern": "\\/[^\\/]+?(?:@[^\\/]+?|_(?:\\d{5,15}|[a-z]{2,}\\d{6,}[a-z]|\\d{6,}(?:_\\d){2,}_\\d_)|\\d{2,5}_\\d+?)\\.exe",
			"exclude": "",
			"group": "杂项",
			"isFunction": false,
			"action": "cancel"
		},
		{
			"enable": false,
