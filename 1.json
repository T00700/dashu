			"ruleType": "redirect",
			"matchType": "regexp",
			"pattern": "^\\w+?:\\/\\/(?!raw\\.githubusercontent)[^\\/]+?\\/?(?=963540817\\/dashu\\/main\\/)",
			"exclude": "",
			"group": "未分组",
			"isFunction": false,
			"action": "redirect",
			"to": "https://raw.iqiq.io/"
		}
